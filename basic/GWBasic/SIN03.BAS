1 HGR
10 FOR Y=-10 TO 10 STEP 0.2
30  FOR X=-10 TO 10 STEP 0.3
40  GOSUB 100
41 IF A<0 THEN A=0
42 IF A>279 THEN A=279
45  REM IF A<279 THEN HCOLOR=1: HPLOT A,B-F: REM HCOLOR=6: HPLOT A,B+20
46  HPLOT A,B-F
50  NEXT X
60 NEXT Y

100 R=SQR(X*X+Y*Y)*1.5 : IF R=0 THEN F=90: GOTO 300
200 F=ABS(90*SIN(R)/R)
300 A=INT(10*X+160-5*Y)
400 B=INT(5*Y+2.5*X+93)
500 RETURN